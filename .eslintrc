{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": { "node": true, "browser": true, "es6": true },
  "plugins": ["jest", "react", "react-hooks"],
  "rules": {
    "no-console": 0,
    "react-hooks/rules-of-hooks": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "_+" }]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
